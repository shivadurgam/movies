<h1>Movies</h1>
<table>
	<thead>
		<tr>
			<th>Movie Name |</th>
			<th>Actor |</th>
			<th>Actress</th>
			<th></th>
			

		</tr>
	</thead>
	<tbody>
		<% @movies.each do |f| %>
			<tr>
				<td><%= f.name %></td>
				<td><%= f.actor %></td>
				<td><%= f.actress %></td>
				<td><%= link_to 'Show'%></td>
			</tr>
		<% end %>
	</tbody>
</table>